package graph;

public class Expression {
	private Guard guard;
	public Expression(Guard g){
		if(g!=null){
			g.addExpression(this);
			this.setGuard(g);
		}
		else{
			Guard g1 = new Guard();
			g1.addExpression(this);
			g = g1;
			this.setGuard(g);
		}
	}
	public Guard getGuard() {
		return guard;
	}
	public void setGuard(Guard guard) {
		this.guard = guard;
	}
}
